structure (Recipe) {
  property (cocktail){
    type(Cocktail)
    min (Required) max(One) 
  }
  property (cookware){
    type(Cookware)
    min (Required) max (Many)
  }
  property (method) {
    type (Method)
    min (Required) max (One)
  }
  property (recipeImageUrl) { //칵테일제조 이미지[0] 
    type (RecipeImageUrl)
    min (Required) max (Many)
  }
  property(steps){
    type(Steps)
    min (Required) max (Many)
  }
}