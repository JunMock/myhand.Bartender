layout-macro-def(FindCategory) {
  params {
    param (category) {
			type (Category)
			min (Required) max (Many)
		}
  }

  content {
    section {  // this macro returns a "section"
      content {
        paragraph {
          value ("#{value(__PARAM_NAME__)}")
          style (Detail_M)
        }
      }
    }
  }
}
